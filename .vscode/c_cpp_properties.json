{
    "configurations": [
        {
            "name": "WSL",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include/boost",
                "/usr/include/**"
            ],
            "defines": [ 
                "__cpp_concepts=201907L",
                "__cplusplus=201907L"
                
            ],
            
            "browse": {
                "databaseFilename": "${default}",
                "limitSymbolsToIncludedHeaders": true,
                "path": [
                    "${workspaceFolder}/**",
                    "/usr/include/boost",
                    "/usr/include/**"
                ]
            },
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c11",
            "cppStandard":"gnu++20",
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}